UI_Path=UI

clear

echo -e "\e[1;37mGenerating UIs..."
echo -e "\e[1;31m   Generating MainUI"
pyuic4 -o $UI_Path/MainUI/MainUI.py $UI_Path/MainUI/MainUI.ui

echo -e "\e[1;31m   Generating TrainUI"
pyuic4 -o $UI_Path/TrainUI/GridUI.py $UI_Path/TrainUI/GridUI.ui

echo -e "\e[1;37m\nStarting Interface\n\e[0m"
python GUI.py
